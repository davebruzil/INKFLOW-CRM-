# INKFLOW CRM Frontend Environment Variables Template
# Copy this file to .env and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Google Sheets Configuration
VITE_GOOGLE_PROJECT_ID=your-google-project-id
VITE_GOOGLE_PRIVATE_KEY_ID=your-private-key-id
VITE_GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
VITE_GOOGLE_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
VITE_GOOGLE_CLIENT_ID=your-client-id
VITE_GOOGLE_SHEETS_ID=your-google-sheets-id

# Database Name
VITE_DATABASE_NAME=inkflow_crm

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:3001
